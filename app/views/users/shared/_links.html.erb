<%- if controller_name != 'sessions' %>
  <fieldset class='form-group mb-0 small-btm'>
    <%= link_to 'Log in', new_session_path(resource_name) %>
  </fieldset>
<% end -%>
<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <fieldset class='form-group mb-0 small-btm'>
    <%= link_to 'Forgot password?', new_password_path(resource_name) %>
   </fieldset>
<% end -%>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <fieldset class='form-group mb-0 small-btm'>
    <%= link_to 'Do not have an account? Register now', new_registration_path(resource_name) %>
  </fieldset>
<% end -%>
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <fieldset class='form-group mb-0 small-btm'>
    <%= link_to 'Did not receive confirmation instructions?', new_confirmation_path(resource_name) %><br />
  </fieldset>
<% end -%>
<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to 'Did not receive unlock instructions?', new_unlock_path(resource_name) %>
<% end -%>
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to 'Sign in with #{provider.to_s.titleize}', omniauth_authorize_path(resource_name, provider) %>
  <% end -%>
<% end -%>
