<div class='dashboard-body'>

  <%= render 'show_group_details' %>

  <div>
    <br>
    <table class="table table-striped table-hover ">
      <tr>
        <th>Name </th>
        <th>Description</th>
        <th>Action</th>
      </tr>
      <% @members.each do |member| %>
        <tr>
          <td> <%= member.name %> </td>
          <td> <%= member.email %> </td>
          <td>
            <%= link_to 'view', user_path(member) %>
          </td>
        </tr>
      <% end %>
    </table>
    <%= paginate @members %>
  </div>
</div>
<div class='dashboard-sidebar'>
  <%= render 'homes/group' %>
</div>